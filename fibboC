#include <stdio.h>

int main(void) {
	int n=10;
	//printf("ingrese por favor la posicion del numero de fibonacci deseado: ");
	//scanf("%d",&n);
	int a = 0;
	printf("1: "+a);
	int b = 1;
	printf("2: "+b);
	int cont = 3;
	int c=0;
	for(int i=0;i<n;i++){
		c = a + b;
		a = b;
		b = c;
		printf(cont+": "+c);
	}
	return 0;
}
