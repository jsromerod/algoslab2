
#include <stdio.h>

int main() {
	long long n=400;
	long long a = 0;
	printf("1: %d \n",a);
	long long b = 1;
	printf("2: %d \n",b);
	long long cont = 3;
	long long c=0;
	for(int i=0;i<n;i++){
		c = a + b;
		a = b;
		b = c;
		printf("%d: %d \n",cont,c);
		cont++;
	}
	return 0;
}

// maximo numero 47 con long long
